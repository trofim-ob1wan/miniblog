{% extends 'base.html.twig' %}

{% block title %}Create new post{% endblock %}

{% block body %}
<div class="row mt-5">
    <div class="col">
        {{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.name) }}
        {{ form_row(form.description) }}
        {{ form_row(form.img, {'required': true}) }}
        {{ form_end(form) }}
    </div>
</div>

{% endblock %}

